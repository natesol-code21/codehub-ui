{
    "family": "codehub-ui",
    "networkMode": "awsvpc",
    "containerDefinitions": [
        {
            "image": "927373803645.dkr.ecr.us-east-1.amazonaws.com/nate-docker-production/codehub-ui:latest",
            "name": "codehub-ui",
            "portMappings": [
                {
                    "containerPort": 80,
                    "hostPort": 0,
                    "protocol": "tcp"
                }
            ],
            "essential": true,
            "entryPoint": [
                "sh",
                "-c",
                "./app/entrypoint.sh"
            ],
            "command": [
                "/bin/sh -c \"'nginx', '-g', 'daemon off;'\""
            ],
            "requiresCompatibilities": [
                "FARGATE"
            ],
            "cpu": "256",
            "memory": "512",
            "executionRoleArn": "arn:aws:iam::927373803645:role/codehub-admin"

        }
    ]
}
